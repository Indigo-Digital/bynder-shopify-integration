{
	"name": "bynder-integration",
	"private": true,
	"scripts": {
		"build": "react-router build",
		"dev": "shopify app dev",
		"config:link": "shopify app config link",
		"generate": "shopify app generate",
		"deploy": "shopify app deploy",
		"config:use": "shopify app config use",
		"env": "shopify app env",
		"start": "react-router-serve ./build/server/index.js",
		"worker": "tsx app/lib/sync/worker.ts",
		"docker-start": "pnpm run setup && pnpm run start",
		"setup": "sh scripts/setup-db.sh",
		"setup:postgres": "cp prisma/schema.postgresql.prisma prisma/schema.prisma && pnpm run setup",
		"lint": "biome check .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"shopify": "shopify",
		"prisma": "prisma",
		"graphql-codegen": "graphql-codegen",
		"vite": "vite",
		"typecheck": "react-router typegen && tsc --noEmit",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage"
	},
	"type": "module",
	"engines": {
		"node": ">=20.19 <22 || >=22.12"
	},
	"dependencies": {
		"@bynder/bynder-js-sdk": "^2.5.2",
		"@prisma/client": "^6.19.0",
		"@react-router/dev": "^7.9.3",
		"@react-router/fs-routes": "^7.9.3",
		"@react-router/node": "^7.9.3",
		"@react-router/serve": "^7.9.3",
		"@shopify/app-bridge-react": "^4.2.4",
		"@shopify/shopify-app-react-router": "^1.0.0",
		"@shopify/shopify-app-session-storage-prisma": "^7.0.0",
		"axios": "^1.13.2",
		"form-data": "^4.0.5",
		"isbot": "^5.1.31",
		"p-map": "^7.0.4",
		"prisma": "^6.19.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router": "^7.9.3",
		"tsx": "^4.20.6",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.6",
		"@shopify/api-codegen-preset": "^1.2.0",
		"@shopify/polaris-types": "^1.0.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^22.18.8",
		"@types/react": "^18.3.25",
		"@types/react-dom": "^18.3.7",
		"@vitest/coverage-v8": "^4.0.12",
		"@vitest/ui": "^4.0.12",
		"graphql-config": "^5.1.1",
		"jsdom": "^27.2.0",
		"typescript": "^5.9.3",
		"vite": "^6.3.6",
		"vitest": "^4.0.12"
	},
	"workspaces": [
		"extensions/*"
	],
	"trustedDependencies": [
		"@shopify/plugin-cloudflare"
	],
	"overrides": {
		"p-map": "^4.0.0"
	},
	"author": "kev0c"
}
